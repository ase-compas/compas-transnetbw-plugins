    "@smui/button": "^8.0.0",
    "@smui/card": "^8.0.0",
    "@smui/data-table": "^8.0.0",
    "@smui/dialog": "^8.0.0",
    "@smui/icon-button": "^8.0.0",
    "@smui/linear-progress": "^8.0.0",
    "@smui/paper": "^8.0.0",
    "@smui/select": "^8.0.0",
    "@smui/textfield": "^8.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
