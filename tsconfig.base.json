{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@oscd-transnet-plugins/oscd-archive-explorer": [
        "libs/oscd-archive-explorer/src/index.ts"
      ],
      "@oscd-transnet-plugins/oscd-archiving-api-client": [
        "libs/oscd-archiving-api-client/src/index.ts"
      ],
      "@oscd-transnet-plugins/oscd-component": [
        "libs/oscd-component/src/index.ts"
      ],
      "@oscd-transnet-plugins/oscd-history-api-client": [
        "libs/oscd-history-api-client/src/index.ts"
      ],
      "@oscd-transnet-plugins/oscd-history-viewer": [
        "libs/oscd-history-viewer/src/index.ts"
      ],
      "@oscd-transnet-plugins/oscd-location-manager": [
        "libs/oscd-location-manager/src/index.ts"
      ],
      "@oscd-transnet-plugins/oscd-location-viewer": [
        "libs/oscd-location-viewer/src/index.ts"
      ],
      "@oscd-transnet-plugins/oscd-icons": [
        "libs/oscd-icons/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
